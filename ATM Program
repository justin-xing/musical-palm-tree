import java.io.*;
public class name{
  public static void main (String[] args)throws IOException{
    System.out.println ("Hello. What value of bills would you like to withdraw?");
    BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
    int money;
    money = Integer.parseInt (br. readLine () ) ;
    System.out.println ("How many dollar bills?");
    int bills;
    bills = Integer.parseInt (br. readLine () ) ;
    System.out.println ("Okay, you have withdrawn " + bills + " $" + money + " bills");
    int money2 = 0;
      while (money2 < bills){
      System.out.println (money + " dollar bill");
        money2++;
      }
  }
}
